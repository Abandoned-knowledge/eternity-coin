generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model transactions {
  transactions_id     Int       @id @default(autoincrement())
  user_id             Int?
  category_id         Int?
  value               Int?
  transaction_type_id Int?
  date                DateTime? @db.DateTime(0)
}
